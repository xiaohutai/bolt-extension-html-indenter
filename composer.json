{
    "name": "xiaohutai/html-indenter",
    "description": "Indent HTML responses. For aesthetics.",
    "type": "bolt-extension",
    "keywords": [
        "html",
        "aesthetics",
        "minify",
        "indent",
        "experimental",
        "proof-of-concept"
    ],
    "require": {
        "bolt/bolt": "^3.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Xiao-Hu Tai",
            "email": "xiao@twokings.nl"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Bolt\\Extension\\XiaoHuTai\\HtmlIndenter\\": "src"
        }
    },
    "extra": {
        "bolt-class": "Bolt\\Extension\\XiaoHuTai\\HtmlIndenter\\HtmlIndenterExtension",
        "bolt-icon": "bolt-icon.png"
    }
}
